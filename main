        minimizedButtonColor = Color3.fromRGB(0, 120, 215),
        minimizedButtonTransparency = 0.3,
        
        -- –ù–∞—Å—Ç—Ä–æ–π–∫–∏ –Ω–µ–≤–∏–¥–∏–º–æ—Å—Ç–∏
