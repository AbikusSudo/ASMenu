        isDragging = false,
        dragStart = nil,
        startPos = nil,
        dragInput = nil,
        dragThreshold = 10,
        
        -- –ù–µ–≤–∏–¥–∏–º–æ—Å—Ç—å
        invisibilityCooldownRemaining = 0,
        invisibilityDurationRemaining = 0,
    }
    
    ------------------------------------------------------------
    -- –ú–û–î–£–õ–¨ UI: –°–û–ó–î–ê–ù–ò–ï –í–°–ï–• –≠–õ–ï–ú–ï–ù–¢–û–í –ò–ù–¢–ï–†–§–ï–ô–°–ê
    ------------------------------------------------------------
    local UI = {}
    
    -- –û—Å–Ω–æ–≤–Ω–æ–π ScreenGui
    UI.screenGui = createUI("ScreenGui", {
        Name = "ASMenu",
        ResetOnSpawn = false,
        DisplayOrder = 999,
    })
    UI.screenGui.Parent = playerGui
    
    -- –ì–õ–ê–í–ù–û–ï –ú–ï–ù–Æ
    UI.mainFrame = createUI("Frame", {
        Name = "MainFrame",
        Size = UDim2.new(0, config.menuWidth, 0, config.menuHeight),
        Position = UDim2.new(0, 20, 0.5, -config.menuHeight/2),
        BackgroundColor3 = Color3.fromRGB(45,45,45),
        BorderSizePixel = 0,
        Active = true,
        Selectable = true,
        Visible = true,
    })
    UI.mainFrame.Parent = UI.screenGui
    createUI("UICorner", { CornerRadius = UDim.new(0, 12), Parent = UI.mainFrame })
    
    -- –°–æ–∑–¥–∞—ë–º —ç—Ñ—Ñ–µ–∫—Ç —Ç–µ–Ω–∏ –¥–ª—è –º–µ–Ω—é
    local shadow = createUI("ImageLabel", {
        Name = "Shadow",
        Size = UDim2.new(1, 10, 1, 10),
        Position = UDim2.new(0, -5, 0, -5),
        BackgroundTransparency = 1,
        Image = "rbxassetid://5554236805",
        ImageColor3 = Color3.fromRGB(0, 0, 0),
        ImageTransparency = 0.8,
        ScaleType = Enum.ScaleType.Slice,
        SliceCenter = Rect.new(23,23,277,277),
    })
    shadow.Parent = UI.mainFrame
    
    -- –ó–ê–ì–û–õ–û–í–û–ö –ú–ï–ù–Æ –° –ö–ù–û–ü–ö–ê–ú–ò –£–ü–†–ê–í–õ–ï–ù–ò–Ø
    UI.titleBar = createUI("Frame", {
        Name = "TitleBar",
        Size = UDim2.new(1, 0, 0, config.titleBarHeight),
        BackgroundColor3 = Color3.fromRGB(35,35,35),
        BorderSizePixel = 0,
        Active = true,
        Selectable = true,
    })
    UI.titleBar.Parent = UI.mainFrame
    createUI("UICorner", { CornerRadius = UDim.new(0, 12, 0, 0), Parent = UI.titleBar })
    
    -- –¢–µ–∫—Å—Ç –∑–∞–≥–æ–ª–æ–≤–∫–∞
    UI.titleText = createUI("TextLabel", {
        Text = "üì± ASMenu vF1",
        Size = UDim2.new(1, -100, 1, 0),
        Position = UDim2.new(0, 10, 0, 0),
        BackgroundTransparency = 1,
        TextColor3 = Color3.fromRGB(255,255,255),
        TextSize = 16,
        Font = Enum.Font.GothamBold,
        TextXAlignment = Enum.TextXAlignment.Left,
    })
    UI.titleText.Parent = UI.titleBar
    
    -- –ö–Ω–æ–ø–∫–∞ –ø–µ—Ä–µ–º–µ—â–µ–Ω–∏—è (–∏–∫–æ–Ω–∫–∞ –ø–µ—Ä–µ–º–µ—â–µ–Ω–∏—è)
    UI.moveButton = createUI("TextButton", {
        Name = "MoveButton",
        Size = UDim2.new(0, 30, 0, 30),
        Position = UDim2.new(1, -70, 0.5, -15),
        BackgroundColor3 = Color3.fromRGB(65,65,65),
        BorderSizePixel = 0,
        Text = "‚ÜîÔ∏è",
        TextSize = 14,
        Active = true,
        Selectable = true,
    })
    UI.moveButton.Parent = UI.titleBar
    createUI("UICorner", { CornerRadius = UDim.new(1, 0), Parent = UI.moveButton })
    
    -- –ö–Ω–æ–ø–∫–∞ —Å–≤–æ—Ä–∞—á–∏–≤–∞–Ω–∏—è –≤ –º–∞–ª–µ–Ω—å–∫—É—é –∫–Ω–æ–ø–æ—á–∫—É
    UI.minimizeButton = createUI("TextButton", {
        Name = "MinimizeButton",
        Size = UDim2.new(0, 30, 0, 30),
        Position = UDim2.new(1, -35, 0.5, -15),
        BackgroundColor3 = Color3.fromRGB(65,65,65),
        BorderSizePixel = 0,
        Text = "‚¨áÔ∏è",
        TextSize = 14,
        Active = true,
        Selectable = true,
    })
    UI.minimizeButton.Parent = UI.titleBar
    createUI("UICorner", { CornerRadius = UDim.new(1, 0), Parent = UI.minimizeButton })
    
    -- –ö–û–ù–¢–ï–ô–ù–ï–† –î–õ–Ø –≠–õ–ï–ú–ï–ù–¢–û–í –ú–ï–ù–Æ
    UI.contentFrame = createUI("ScrollingFrame", {
        Name = "ContentFrame",
        Size = UDim2.new(1, 0, 1, -config.titleBarHeight),
        Position = UDim2.new(0, 0, 0, config.titleBarHeight),
        BackgroundTransparency = 1,
        BorderSizePixel = 0,
        ScrollBarThickness = 6,
        ScrollBarImageColor3 = Color3.fromRGB(100,100,100),
        CanvasSize = UDim2.new(0, 0, 0, 0),
    })
    UI.contentFrame.Parent = UI.mainFrame
    
    UI.contentListLayout = createUI("UIListLayout", {
        Padding = UDim.new(0, 8),
        SortOrder = Enum.SortOrder.LayoutOrder,
        HorizontalAlignment = Enum.HorizontalAlignment.Center,
    })
    UI.contentListLayout.Parent = UI.contentFrame
    
    UI.contentPadding = createUI("UIPadding", {
        PaddingTop = UDim.new(0, 10),
        PaddingBottom = UDim.new(0, 10),
        PaddingLeft = UDim.new(0, 10),
        PaddingRight = UDim.new(0, 10),
    })
    UI.contentPadding.Parent = UI.contentFrame
    
    UI.contentListLayout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
        UI.contentFrame.CanvasSize = UDim2.new(0, 0, 0, UI.contentListLayout.AbsoluteContentSize.Y + 20)
    end)
    
    -- –§—É–Ω–∫—Ü–∏—è —Å–æ–∑–¥–∞–Ω–∏—è —Å—Ç–∞–Ω–¥–∞—Ä—Ç–Ω–æ–π –∫–Ω–æ–ø–∫–∏
    function UI.createButton(name, text, order, icon)
        local btn = createUI("TextButton", {
            Name = name,
            Size = UDim2.new(1, -20, 0, 45),
            BackgroundColor3 = Color3.fromRGB(65,65,65),
            BorderSizePixel = 0,
            TextColor3 = Color3.fromRGB(255,255,255),
            TextSize = 14,
            Font = Enum.Font.Gotham,
            Text = (icon or "") .. " " .. text,
            Active = true,
            Selectable = true,
            AutoButtonColor = true,
        })
        btn.LayoutOrder = order
        btn.Parent = UI.contentFrame
        
        createUI("UICorner", { CornerRadius = UDim.new(0, 8), Parent = btn })
        
        -- –≠—Ñ—Ñ–µ–∫—Ç –Ω–∞–∂–∞—Ç–∏—è
        btn.MouseButton1Down:Connect(function()
            btn.BackgroundColor3 = Color3.fromRGB(85,85,85)
        end)
        
        btn.MouseButton1Up:Connect(function()
            btn.BackgroundColor3 = Color3.fromRGB(65,65,65)
        end)
        
        btn.MouseLeave:Connect(function()
            btn.BackgroundColor3 = Color3.fromRGB(65,65,65)
        end)
        
        return btn
    end
    
    -- –°–û–ó–î–ê–ù–ò–ï –ö–ù–û–ü–û–ö –ú–ï–ù–Æ
    -- –û—Å–Ω–æ–≤–Ω—ã–µ —Ñ—É–Ω–∫—Ü–∏–∏
    UI.teleportPlayersButton = UI.createButton("TeleportPlayersButton", "–¢–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏—è –∫ –∏–≥—Ä–æ–∫–∞–º", 1, "üìç")
    UI.noClipButton = UI.createButton("NoClipButton", "–•–æ–¥–∏—Ç—å —Å–∫–≤–æ–∑—å —Å—Ç–µ–Ω—ã", 2, "üëª")
    UI.flyButton = UI.createButton("FlyButton", "–ü–æ–ª—ë—Ç", 3, "‚úàÔ∏è")
    UI.noFallDamageButton = UI.createButton("NoFallDamageButton", "–ù–µ—Ç —É—Ä–æ–Ω–∞ –æ—Ç –ø–∞–¥–µ–Ω–∏—è", 4, "üõ°Ô∏è")
    
    -- –ù–û–í–ê–Ø –ö–ù–û–ü–ö–ê: –°–ï–†–í–ï–†–ù–ê–Ø –ù–ï–í–ò–î–ò–ú–û–°–¢–¨
    UI.invisibilityButton = UI.createButton("InvisibilityButton", "–°–µ—Ä–≤–µ—Ä–Ω–∞—è –Ω–µ–≤–∏–¥–∏–º–æ—Å—Ç—å", 5, "üëÅÔ∏è")
    
    -- ESP —Ñ—É–Ω–∫—Ü–∏–∏
    UI.espPlayersButton = UI.createButton("ESPPlayersButton", "ESP –ò–≥—Ä–æ–∫–æ–≤", 6, "üë§")
    UI.espNPCButton = UI.createButton("ESPNPCButton", "ESP –°—É—â–µ—Å—Ç–≤", 7, "üëπ")
    
    -- –û—Å–≤–µ—â–µ–Ω–∏–µ
    UI.lightButton = UI.createButton("LightButton", "–û—Å–≤–µ—Ç–∏—Ç—å –∫–∞—Ä—Ç—É", 8, "üí°")
    
    -- –ù–∞—Å—Ç—Ä–æ–π–∫–∏ –ø–µ—Ä—Å–æ–Ω–∞–∂–∞
    UI.speedButton = UI.createButton("SpeedButton", "–°–∫–æ—Ä–æ—Å—Ç—å: " .. state.currentSpeed, 9, "‚ö°")
    UI.jumpButton = UI.createButton("JumpButton", "–ü—Ä—ã–∂–æ–∫: " .. state.currentJump, 10, "ü¶ò")
    
    -- –í–∏–¥ –æ—Ç —Ç—Ä–µ—Ç—å–µ–≥–æ –ª–∏—Ü–∞
    UI.thirdPersonButton = UI.createButton("ThirdPersonButton", "–†–µ–∂–∏–º 3-–≥–æ –ª–∏—Ü–∞", 11, "üé•")
    
    ------------------------------------------------------------
    -- –ú–ê–õ–ï–ù–¨–ö–ê–Ø –ö–ù–û–ü–û–ß–ö–ê –î–õ–Ø –û–¢–ö–†–´–¢–ò–Ø –ú–ï–ù–Æ
    ------------------------------------------------------------
    UI.minimizedButton = createUI("TextButton", {
        Name = "MinimizedButton",
        Size = config.minimizedButtonSize,
        Position = UDim2.new(0, 20, 0.5, -25),
        BackgroundColor3 = config.minimizedButtonColor,
        BackgroundTransparency = config.minimizedButtonTransparency,
        BorderSizePixel = 0,
        Text = "üì±",
        TextSize = 20,
        Visible = false,
        Active = true,
        Selectable = true,
    })
    UI.minimizedButton.Parent = UI.screenGui
    createUI("UICorner", { CornerRadius = UDim.new(1, 0), Parent = UI.minimizedButton })
    
    -- –¢–µ–Ω—å –¥–ª—è –∫–Ω–æ–ø–∫–∏
    local buttonShadow = createUI("ImageLabel", {
        Name = "ButtonShadow",
        Size = UDim2.new(1, 10, 1, 10),
        Position = UDim2.new(0, -5, 0, -5),
        BackgroundTransparency = 1,
        Image = "rbxassetid://5554236805",
        ImageColor3 = Color3.fromRGB(0, 0, 0),
        ImageTransparency = 0.7,
        ScaleType = Enum.ScaleType.Slice,
        SliceCenter = Rect.new(23,23,277,277),
    })
    buttonShadow.Parent = UI.minimizedButton
    
    ------------------------------------------------------------
    -- –ú–û–î–£–õ–¨: –ü–ï–†–ï–ú–ï–©–ï–ù–ò–ï –ú–ï–ù–Æ –ò –ö–ù–û–ü–ö–ò
    ------------------------------------------------------------
    local function setupDragging(frame, isMinimized)
        frame.InputBegan:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.Touch or input.UserInputType == Enum.UserInputType.MouseButton1 then
                state.isDragging = true
                state.dragStart = input.Position
                state.startPos = frame.Position
                state.dragInput = input
                
                -- –í–∏–∑—É–∞–ª—å–Ω–∞—è –æ–±—Ä–∞—Ç–Ω–∞—è —Å–≤—è–∑—å
                if frame == UI.mainFrame then
                    frame.BackgroundColor3 = Color3.fromRGB(55,55,55)
                elseif frame == UI.minimizedButton then
                    frame.BackgroundTransparency = config.minimizedButtonTransparency - 0.2
                end
            end
        end)
        
        UserInputService.InputChanged:Connect(function(input)
            if state.isDragging and input == state.dragInput then
                local delta = input.Position - state.dragStart
                
                -- –ü—Ä–∏–º–µ–Ω—è–µ–º –ø–µ—Ä–µ–º–µ—â–µ–Ω–∏–µ —Ç–æ–ª—å–∫–æ –µ—Å–ª–∏ –ø—Ä–µ–≤—ã—à–µ–Ω –ø–æ—Ä–æ–≥
                if delta.Magnitude > state.dragThreshold then
                    local newX = state.startPos.X.Offset + delta.X
                    local newY = state.startPos.Y.Offset + delta.Y
                    
                    -- –û–≥—Ä–∞–Ω–∏—á–∏–≤–∞–µ–º –ø–æ–ª–æ–∂–µ–Ω–∏–µ –≤ –ø—Ä–µ–¥–µ–ª–∞—Ö —ç–∫—Ä–∞–Ω–∞
                    local screenSize = playerGui.AbsoluteSize
                    local frameSize = frame.AbsoluteSize
                    
                    newX = math.clamp(newX, 0, screenSize.X - frameSize.X)
                    newY = math.clamp(newY, 0, screenSize.Y - frameSize.Y)
                    
                    frame.Position = UDim2.new(0, newX, 0, newY)
                end
            end
        end)
        
        UserInputService.InputEnded:Connect(function(input)
            if input == state.dragInput then
                state.isDragging = false
                state.dragInput = nil
                
                -- –í–æ—Å—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º —Ü–≤–µ—Ç
                if frame == UI.mainFrame then
                    frame.BackgroundColor3 = Color3.fromRGB(45,45,45)
                elseif frame == UI.minimizedButton then
                    frame.BackgroundTransparency = config.minimizedButtonTransparency
                end
            end
        end)
    end
    
    -- –ù–∞—Å—Ç—Ä–∞–∏–≤–∞–µ–º –ø–µ—Ä–µ–º–µ—â–µ–Ω–∏–µ –¥–ª—è –æ—Å–Ω–æ–≤–Ω–æ–≥–æ –º–µ–Ω—é –∏ –∫–Ω–æ–ø–∫–∏
    setupDragging(UI.mainFrame, false)
    setupDragging(UI.minimizedButton, true)
    
    ------------------------------------------------------------
    -- –ú–û–î–£–õ–¨: –ü–ï–†–ï–ö–õ–Æ–ß–ï–ù–ò–ï –ú–ï–ñ–î–£ –ú–ï–ù–Æ –ò –ö–ù–û–ü–ö–û–ô
    ------------------------------------------------------------
    UI.minimizeButton.MouseButton1Click:Connect(function()
        -- –°–æ—Ö—Ä–∞–Ω—è–µ–º –ø–æ–∑–∏—Ü–∏—é –º–µ–Ω—é
        local menuPos = UI.mainFrame.Position
        
        -- –°–∫—Ä—ã–≤–∞–µ–º –º–µ–Ω—é, –ø–æ–∫–∞–∑—ã–≤–∞–µ–º –∫–Ω–æ–ø–∫—É
        UI.mainFrame.Visible = false
        UI.minimizedButton.Position = menuPos
        UI.minimizedButton.Visible = true
        
        state.menuVisible = false
    end)
    
    UI.minimizedButton.MouseButton1Click:Connect(function()
        -- –ï—Å–ª–∏ –±—ã–ª–æ –ø–µ—Ä–µ–º–µ—â–µ–Ω–∏–µ, –Ω–µ –æ—Ç–∫—Ä—ã–≤–∞–µ–º –º–µ–Ω—é
        if state.isDragging then return end
        
        -- –°–æ—Ö—Ä–∞–Ω—è–µ–º –ø–æ–∑–∏—Ü–∏—é –∫–Ω–æ–ø–∫–∏
        local buttonPos = UI.minimizedButton.Position
        
        -- –°–∫—Ä—ã–≤–∞–µ–º –∫–Ω–æ–ø–∫—É, –ø–æ–∫–∞–∑—ã–≤–∞–µ–º –º–µ–Ω—é
        UI.minimizedButton.Visible = false
        UI.mainFrame.Position = buttonPos
        UI.mainFrame.Visible = true
        
        state.menuVisible = true
    end)
    
    ------------------------------------------------------------
    -- –ú–û–î–£–õ–¨: –ö–ù–û–ü–ö–ê –ü–ï–†–ï–ú–ï–©–ï–ù–ò–Ø (–î–õ–Ø –ú–û–ë–ò–õ–¨–ù–´–• –£–°–¢–†–û–ô–°–¢–í)
    ------------------------------------------------------------
    UI.moveButton.MouseButton1Click:Connect(function()
        -- –ü–µ—Ä–µ–∫–ª—é—á–∞–µ–º —Ä–µ–∂–∏–º –ø–µ—Ä–µ–º–µ—â–µ–Ω–∏—è
        if state.isDragging then
            state.isDragging = false
            UI.moveButton.Text = "‚ÜîÔ∏è"
        else
            state.isDragging = true
            UI.moveButton.Text = "üéØ"
        end
    end)
    
    ------------------------------------------------------------
    -- –ú–û–î–£–õ–¨: –°–ï–†–í–ï–†–ù–ê–Ø –ù–ï–í–ò–î–ò–ú–û–°–¢–¨ (REAL SERVER-SIDE INVISIBILITY)
    ------------------------------------------------------------
    local function setupServerSideInvisibility()
        -- –ü—Ä–æ–≤–µ—Ä—è–µ–º, —Å—É—â–µ—Å—Ç–≤—É–µ—Ç –ª–∏ —É–∂–µ RemoteEvent –¥–ª—è –Ω–µ–≤–∏–¥–∏–º–æ—Å—Ç–∏
        local invisRemote
        if ReplicatedStorage:FindFirstChild("InvisibilityRemote") then
            invisRemote = ReplicatedStorage.InvisibilityRemote
        else
            -- –°–æ–∑–¥–∞–µ–º RemoteEvent –¥–ª—è –∫–æ–º–º—É–Ω–∏–∫–∞—Ü–∏–∏ —Å —Å–µ—Ä–≤–µ—Ä–æ–º
            invisRemote = Instance.new("RemoteEvent")
            invisRemote.Name = "InvisibilityRemote"
            invisRemote.Parent = ReplicatedStorage
        end
        
        -- –§—É–Ω–∫—Ü–∏—è –∞–∫—Ç–∏–≤–∞—Ü–∏–∏ –Ω–µ–≤–∏–¥–∏–º–æ—Å—Ç–∏
        local function activateInvisibility()
            if state.invisibilityCooldownRemaining > 0 then
                -- –ü–æ–∫–∞–∑—ã–≤–∞–µ–º —Å–æ–æ–±—â–µ–Ω–∏–µ –æ –ø–µ—Ä–µ–∑–∞—Ä—è–¥–∫–µ
                local originalText = UI.invisibilityButton.Text
                UI.invisibilityButton.Text = "‚è≥ " .. math.ceil(state.invisibilityCooldownRemaining) .. "s"
                
                task.wait(1)
                UI.invisibilityButton.Text = originalText
                return
            end
            
            if state.isInvisible then
                -- –î–µ–∞–∫—Ç–∏–≤–∞—Ü–∏—è –Ω–µ–≤–∏–¥–∏–º–æ—Å—Ç–∏
                invisRemote:FireServer("Deactivate")
                state.isInvisible = false
                state.invisibilityDurationRemaining = 0
                UI.invisibilityButton.Text = "üëÅÔ∏è –°–µ—Ä–≤–µ—Ä–Ω–∞—è –Ω–µ–≤–∏–¥–∏–º–æ—Å—Ç—å"
                UI.invisibilityButton.BackgroundColor3 = Color3.fromRGB(65,65,65)
            else
                -- –ê–∫—Ç–∏–≤–∞—Ü–∏—è –Ω–µ–≤–∏–¥–∏–º–æ—Å—Ç–∏
                invisRemote:FireServer("Activate", config.invisibilityDuration)
                state.isInvisible = true
                state.invisibilityDurationRemaining = config.invisibilityDuration
                state.invisibilityCooldownRemaining = config.invisibilityCooldown + config.invisibilityDuration
                UI.invisibilityButton.Text = "üëÅÔ∏è –ù–µ–≤–∏–¥–∏–º " .. config.invisibilityDuration .. "s"
                UI.invisibilityButton.BackgroundColor3 = Color3.fromRGB(0, 150, 0)
                
                -- –ó–∞–ø—É—Å–∫–∞–µ–º —Ç–∞–π–º–µ—Ä –¥–ª–∏—Ç–µ–ª—å–Ω–æ—Å—Ç–∏
                spawn(function()
                    while state.invisibilityDurationRemaining > 0 and state.isInvisible do
                        task.wait(1)
                        state.invisibilityDurationRemaining = state.invisibilityDurationRemaining - 1
                        UI.invisibilityButton.Text = "üëÅÔ∏è –ù–µ–≤–∏–¥–∏–º " .. state.invisibilityDurationRemaining .. "s"
                    end
                    
                    if state.invisibilityDurationRemaining <= 0 and state.isInvisible then
                        -- –ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏ –æ—Ç–∫–ª—é—á–∞–µ–º –Ω–µ–≤–∏–¥–∏–º–æ—Å—Ç—å
                        invisRemote:FireServer("Deactivate")
                        state.isInvisible = false
                        UI.invisibilityButton.Text = "üëÅÔ∏è –°–µ—Ä–≤–µ—Ä–Ω–∞—è –Ω–µ–≤–∏–¥–∏–º–æ—Å—Ç—å"
                        UI.invisibilityButton.BackgroundColor3 = Color3.fromRGB(65,65,65)
                    end
                end)
                
                -- –ó–∞–ø—É—Å–∫–∞–µ–º —Ç–∞–π–º–µ—Ä –ø–µ—Ä–µ–∑–∞—Ä—è–¥–∫–∏
                spawn(function()
                    while state.invisibilityCooldownRemaining > 0 do
                        task.wait(1)
                        state.invisibilityCooldownRemaining = state.invisibilityCooldownRemaining - 1
                    end
                end)
            end
        end
        
        -- –ü–æ–¥–∫–ª—é—á–∞–µ–º –∫–Ω–æ–ø–∫—É
        UI.invisibilityButton.MouseButton1Click:Connect(activateInvisibility)
        
        -- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —É—Å—Ç–∞–Ω–æ–≤–∫–∏ –Ω–µ–≤–∏–¥–∏–º–æ—Å—Ç–∏ –Ω–∞ —Å–µ—Ä–≤–µ—Ä–µ (–Ω—É–∂–µ–Ω —Å–æ–æ—Ç–≤–µ—Ç—Å—Ç–≤—É—é—â–∏–π —Å–µ—Ä–≤–µ—Ä–Ω—ã–π —Å–∫—Ä–∏–ø—Ç)
        local function setupServerScript()
            -- –≠—Ç–æ—Ç –∫–æ–¥ –Ω—É–∂–Ω–æ —Ä–∞–∑–º–µ—Å—Ç–∏—Ç—å –≤ ServerScriptService
            --[[
            local Players = game:GetService("Players")
            local ReplicatedStorage = game:GetService("ReplicatedStorage")
            
            local invisRemote = ReplicatedStorage:WaitForChild("InvisibilityRemote")
            
            invisRemote.OnServerEvent:Connect(function(player, action, duration)
                if action == "Activate" then
                    -- –î–µ–ª–∞–µ–º –ø–µ—Ä—Å–æ–Ω–∞–∂–∞ –Ω–µ–≤–∏–¥–∏–º—ã–º –¥–ª—è –≤—Å–µ—Ö
                    local character = player.Character
                    if character then
                        for _, part in ipairs(character:GetDescendants()) do
                            if part:IsA("BasePart") then
                                part.Transparency = 1
                                part.CanCollide = false
                            elseif part:IsA("Decal") then
                                part.Transparency = 1
                            end
                        end
                        
                        -- –¢–∞–∫–∂–µ —Å–∫—Ä—ã–≤–∞–µ–º –∏–Ω—Å—Ç—Ä—É–º–µ–Ω—Ç—ã –∏ –∞–∫—Å–µ—Å—Å—É–∞—Ä—ã
                        local humanoid = character:FindFirstChild("Humanoid")
                        if humanoid then
                            for _, accessory in ipairs(humanoid:GetAccessories()) do
                                local handle = accessory:FindFirstChild("Handle")
                                if handle then
                                    handle.Transparency = 1
                                end
                            end
                        end
                        
                        -- –ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–æ–µ –æ—Ç–∫–ª—é—á–µ–Ω–∏–µ —á–µ—Ä–µ–∑ duration —Å–µ–∫—É–Ω–¥
                        task.wait(duration or 30)
                        
                        -- –í–æ—Å—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º –≤–∏–¥–∏–º–æ—Å—Ç—å
                        for _, part in ipairs(character:GetDescendants()) do
                            if part:IsA("BasePart") then
                                part.Transparency = 0
                                part.CanCollide = true
                            elseif part:IsA("Decal") then
                                part.Transparency = 0
                            end
                        end
                        
                        if humanoid then
                            for _, accessory in ipairs(humanoid:GetAccessories()) do
                                local handle = accessory:FindFirstChild("Handle")
                                if handle then
                                    handle.Transparency = 0
                                end
                            end
                        end
                    end
                elseif action == "Deactivate" then
                    -- –ù–µ–º–µ–¥–ª–µ–Ω–Ω–æ –≤–æ—Å—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º –≤–∏–¥–∏–º–æ—Å—Ç—å
                    local character = player.Character
                    if character then
                        for _, part in ipairs(character:GetDescendants()) do
                            if part:IsA("BasePart") then
                                part.Transparency = 0
                                part.CanCollide = true
                            elseif part:IsA("Decal") then
                                part.Transparency = 0
                            end
                        end
                        
                        local humanoid = character:FindFirstChild("Humanoid")
                        if humanoid then
                            for _, accessory in ipairs(humanoid:GetAccessories()) do
                                local handle = accessory:FindFirstChild("Handle")
                                if handle then
                                    handle.Transparency = 0
                                end
                            end
                        end
                    end
                end
            end)
            ]]
            
            print("Server-side invisibility script needs to be placed in ServerScriptService")
        end
        
        return invisRemote
    end
    
    -- –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∏—Ä—É–µ–º —Å–∏—Å—Ç–µ–º—É –Ω–µ–≤–∏–¥–∏–º–æ—Å—Ç–∏
    local invisRemote = setupServerSideInvisibility()
    
    ------------------------------------------------------------
    -- –ú–û–î–£–õ–¨: –û–°–ù–û–í–ù–´–ï –§–£–ù–ö–¶–ò–ò –ú–ï–ù–Æ (—Å–æ—Ö—Ä–∞–Ω–µ–Ω—ã –∏–∑ –æ—Ä–∏–≥–∏–Ω–∞–ª–∞)
    ------------------------------------------------------------
    
    -- –ó–¥–µ—Å—å –¥–æ–ª–∂–Ω—ã –±—ã—Ç—å –≤—Å–µ –æ—Å—Ç–∞–ª—å–Ω—ã–µ —Ñ—É–Ω–∫—Ü–∏–∏ –∏–∑ –æ—Ä–∏–≥–∏–Ω–∞–ª—å–Ω–æ–≥–æ –∫–æ–¥–∞:
    -- 1. –§—É–Ω–∫—Ü–∏–∏ –æ–±–Ω–æ–≤–ª–µ–Ω–∏—è —Å–∫–æ—Ä–æ—Å—Ç–∏ –∏ –ø—Ä—ã–∂–∫–∞
    -- 2. –°–∏—Å—Ç–µ–º–∞ —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏–∏
    -- 3. NoClip –∏ Fly
    -- 4. NoFallDamage
    -- 5. ESP —Å–∏—Å—Ç–µ–º–∞
    -- 6. –û—Å–≤–µ—â–µ–Ω–∏–µ –∫–∞—Ä—Ç—ã
    -- 7. –†–µ–∂–∏–º 3-–≥–æ –ª–∏—Ü–∞
    -- 8. –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è –ø–µ—Ä—Å–æ–Ω–∞–∂–∞
    
    -- –î–ª—è —ç–∫–æ–Ω–æ–º–∏–∏ –º–µ—Å—Ç–∞ —è –ø–æ–∫–∞–∂—É —Ç–æ–ª—å–∫–æ –ø–æ–¥–∫–ª—é—á–µ–Ω–∏–µ –∫–Ω–æ–ø–æ–∫ –∫ –∑–∞–≥–ª—É—à–∫–∞–º
    -- –í —Ä–µ–∞–ª—å–Ω–æ–º –∫–æ–¥–µ –∑–¥–µ—Å—å –¥–æ–ª–∂–Ω—ã –±—ã—Ç—å –ø–æ–ª–Ω—ã–µ —Ä–µ–∞–ª–∏–∑–∞—Ü–∏–∏
    
    local function toggleNoClip()
        state.isNoClip = not state.isNoClip
        UI.noClipButton.BackgroundColor3 = state.isNoClip and Color3.fromRGB(0,150,0) or Color3.fromRGB(65,65,65)
        UI.noClipButton.Text = state.isNoClip and "üëª NoClip –í–ö–õ" or "üëª –•–æ–¥–∏—Ç—å —Å–∫–≤–æ–∑—å —Å—Ç–µ–Ω—ã"
        
        -- –ó–¥–µ—Å—å –¥–æ–ª–∂–Ω–∞ –±—ã—Ç—å —Ä–µ–∞–ª–∏–∑–∞—Ü–∏—è NoClip
        if state.isNoClip then
            print("NoClip activated")
        else
            print("NoClip deactivated")
        end
    end
    
    local function toggleFly()
        state.isFlying = not state.isFlying
        UI.flyButton.BackgroundColor3 = state.isFlying and Color3.fromRGB(0,150,0) or Color3.fromRGB(65,65,65)
        UI.flyButton.Text = state.isFlying and "‚úàÔ∏è –ü–æ–ª—ë—Ç –í–ö–õ" or "‚úàÔ∏è –ü–æ–ª—ë—Ç"
        
        -- –ó–¥–µ—Å—å –¥–æ–ª–∂–Ω–∞ –±—ã—Ç—å —Ä–µ–∞–ª–∏–∑–∞—Ü–∏—è –ø–æ–ª—ë—Ç–∞
        if state.isFlying then
            print("Fly activated")
        else
            print("Fly deactivated")
        end
    end
    
    local function toggleNoFallDamage()
        state.isNoFallDamage = not state.isNoFallDamage
        UI.noFallDamageButton.BackgroundColor3 = state.isNoFallDamage and Color3.fromRGB(0,150,0) or Color3.fromRGB(65,65,65)
        UI.noFallDamageButton.Text = state.isNoFallDamage and "üõ°Ô∏è –ù–µ—Ç —É—Ä–æ–Ω–∞ –í–ö–õ" or "üõ°Ô∏è –ù–µ—Ç —É—Ä–æ–Ω–∞ –æ—Ç –ø–∞–¥–µ–Ω–∏—è"
        
        -- –ó–¥–µ—Å—å –¥–æ–ª–∂–Ω–∞ –±—ã—Ç—å —Ä–µ–∞–ª–∏–∑–∞—Ü–∏—è NoFallDamage
        if state.isNoFallDamage then
            print("NoFallDamage activated")
        else
            print("NoFallDamage deactivated")
        end
    end
    
    -- –ü–æ–¥–∫–ª—é—á–∞–µ–º –æ—Å–Ω–æ–≤–Ω—ã–µ —Ñ—É–Ω–∫—Ü–∏–∏
    UI.noClipButton.MouseButton1Click:Connect(toggleNoClip)
    UI.flyButton.MouseButton1Click:Connect(toggleFly)
    UI.noFallDamageButton.MouseButton1Click:Connect(toggleNoFallDamage)
    
    ------------------------------------------------------------
    -- –ê–î–ê–ü–¢–ò–í–ù–´–ô –ò–ù–¢–ï–†–§–ï–ô–° –î–õ–Ø –ú–û–ë–ò–õ–¨–ù–´–• –£–°–¢–†–û–ô–°–¢–í
    ------------------------------------------------------------
    local function updateMobileUI()
        if UserInputService.TouchEnabled then
            -- –£–≤–µ–ª–∏—á–∏–≤–∞–µ–º —Ä–∞–∑–º–µ—Ä—ã –¥–ª—è —Ç–∞—á-—É–ø—Ä–∞–≤–ª–µ–Ω–∏—è
            config.menuWidth = 240
            config.menuHeight = 350
            config.titleBarHeight = 45
            
            -- –û–±–Ω–æ–≤–ª—è–µ–º —Ä–∞–∑–º–µ—Ä—ã –º–µ–Ω—é
            UI.mainFrame.Size = UDim2.new(0, config.menuWidth, 0, config.menuHeight)
            
            -- –£–≤–µ–ª–∏—á–∏–≤–∞–µ–º –∫–Ω–æ–ø–∫–∏
            for _, button in ipairs(UI.contentFrame:GetChildren()) do
                if button:IsA("TextButton") then
                    button.Size = UDim2.new(1, -20, 0, 50)
                    button.TextSize = 15
                end
            end
            
            -- –£–≤–µ–ª–∏—á–∏–≤–∞–µ–º –∫–Ω–æ–ø–∫—É –º–∏–Ω–∏–º–∏–∑–∞—Ü–∏–∏
            UI.minimizedButton.Size = UDim2.new(0, 60, 0, 60)
            
            print("Mobile UI activated")
        end
    end
    
    -- –ü—Ä–æ–≤–µ—Ä—è–µ–º —Ç–∏–ø —É—Å—Ç—Ä–æ–π—Å—Ç–≤–∞ –ø—Ä–∏ –∑–∞–ø—É—Å–∫–µ
    spawn(updateMobileUI)
    
end