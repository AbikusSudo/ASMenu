-- –ü–æ–ª–Ω–∞—è —Å–∏—Å—Ç–µ–º–∞ –ø—Å–µ–≤–¥–æ-–Ω–µ–≤–∏–¥–∏–º–æ—Å—Ç–∏ —á–µ—Ä–µ–∑ –º–∞–Ω–∏–ø—É–ª—è—Ü–∏—é UI –∏ –º–æ–¥–µ–ª–∏
------------------------------------------------------------

-- –ü–æ–ª—É—á–∞–µ–º —Å–µ—Ä–≤–∏—Å—ã Roblox
local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")
local RunService = game:GetService("RunService")
local UserInputService = game:GetService("UserInputService")
local Lighting = game:GetService("Lighting")
local player = Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

-- –ü—Ä–æ–≤–µ—Ä–∫–∞ –ø–∞—Ä–æ–ª—è
local CORRECT_PASSWORD = "AbikusSudo"
local passwordVerified = false

-- –ï—Å–ª–∏ –º–µ–Ω—é —É–∂–µ —Å—É—â–µ—Å—Ç–≤—É–µ—Ç, –ø—Ä–µ–∫—Ä–∞—â–∞–µ–º –≤—ã–ø–æ–ª–Ω–µ–Ω–∏–µ
if playerGui:FindFirstChild("ASMenu") then
    return
end

-- –ü–æ–∫–∞–∑—ã–≤–∞–µ–º –∑–∞–ø—Ä–æ—Å –ø–∞—Ä–æ–ª—è
local passwordPrompt = Instance.new("ScreenGui")
passwordPrompt.Name = "PasswordPrompt"
passwordPrompt.Parent = playerGui

local passwordFrame = Instance.new("Frame")
passwordFrame.Size = UDim2.new(0, 300, 0, 150)
passwordFrame.Position = UDim2.new(0.5, -150, 0.5, -75)
passwordFrame.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
passwordFrame.BorderSizePixel = 0
passwordFrame.Parent = passwordPrompt

local uiCorner = Instance.new("UICorner")
uiCorner.CornerRadius = UDim.new(0, 8)
uiCorner.Parent = passwordFrame

local title = Instance.new("TextLabel")
title.Text = "ASMenu vF1 - –¢—Ä–µ–±—É–µ—Ç—Å—è –ø–∞—Ä–æ–ª—å"
title.Size = UDim2.new(1, 0, 0, 40)
title.Position = UDim2.new(0, 0, 0, 0)
title.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
title.TextColor3 = Color3.fromRGB(255, 255, 255)
title.Font = Enum.Font.GothamBold
title.TextSize = 16
title.Parent = passwordFrame

local passwordBox = Instance.new("TextBox")
passwordBox.Size = UDim2.new(0.8, 0, 0, 40)
passwordBox.Position = UDim2.new(0.1, 0, 0.3, 0)
passwordBox.BackgroundColor3 = Color3.fromRGB(65, 65, 65)
passwordBox.TextColor3 = Color3.fromRGB(255, 255, 255)
passwordBox.PlaceholderText = "–í–≤–µ–¥–∏—Ç–µ –ø–∞—Ä–æ–ª—å"
passwordBox.Font = Enum.Font.Gotham
passwordBox.TextSize = 14
passwordBox.Parent = passwordFrame

local submitButton = Instance.new("TextButton")
submitButton.Size = UDim2.new(0.8, 0, 0, 40)
submitButton.Position = UDim2.new(0.1, 0, 0.7, 0)
submitButton.BackgroundColor3 = Color3.fromRGB(0, 120, 215)
submitButton.TextColor3 = Color3.fromRGB(255, 255, 255)
submitButton.Text = "–ü–æ–¥—Ç–≤–µ—Ä–¥–∏—Ç—å"
submitButton.Font = Enum.Font.Gotham
submitButton.TextSize = 14
submitButton.Parent = passwordFrame

submitButton.MouseButton1Click:Connect(function()
    if passwordBox.Text == CORRECT_PASSWORD then
        passwordVerified = true
        passwordPrompt:Destroy()
        loadMenu()
    else
        passwordBox.Text = ""
        passwordBox.PlaceholderText = "–ù–µ–≤–µ—Ä–Ω—ã–π –ø–∞—Ä–æ–ª—å!"
    end
end)

-- –û—Å–Ω–æ–≤–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è –∑–∞–≥—Ä—É–∑–∫–∏ –º–µ–Ω—é
local function loadMenu()
    ------------------------------------------------------------
    -- –ö–û–ù–§–ò–ì–£–†–ê–¶–ò–Ø
    ------------------------------------------------------------
    local config = {
        menuWidth = 220,
        menuHeight = 400,
        titleBarHeight = 40,
        creatorName = "AbikusSudo",
        
        -- –ù–∞—Å—Ç—Ä–æ–π–∫–∏ –Ω–µ–≤–∏–¥–∏–º–æ—Å—Ç–∏
        invisibilityOffsetDistance = 10000, -- –î–∏—Å—Ç–∞–Ω—Ü–∏—è —Å–º–µ—â–µ–Ω–∏—è UI
        undergroundHeight = -500, -- –í—ã—Å–æ—Ç–∞ –ø–æ–¥ –∑–µ–º–ª—ë–π –¥–ª—è —Ç–µ–ª–∞
    }
    
    ------------------------------------------------------------
    -- –ì–õ–û–ë–ê–õ–¨–ù–û–ï –°–û–°–¢–û–Ø–ù–ò–ï
    ------------------------------------------------------------
    local state = {
        -- –û—Å–Ω–æ–≤–Ω—ã–µ —Å–æ—Å—Ç–æ—è–Ω–∏—è
        menuVisible = true,
        isDragging = false,
        
        -- –§—É–Ω–∫—Ü–∏–æ–Ω–∞–ª—å–Ω—ã–µ —Å–æ—Å—Ç–æ—è–Ω–∏—è
        isFlying = false,
        isNoClip = false,
        isNoFallDamage = false,
        
        -- –°–∏—Å—Ç–µ–º–∞ –Ω–µ–≤–∏–¥–∏–º–æ—Å—Ç–∏
        isInvisible = false,
        invisibilityData = {
            originalTransparency = {},
            originalCollisions = {},
            originalUIPositions = {},
            originalAccessories = {},
            undergroundPosition = nil,
        },
        
        -- –ù–∞—Å—Ç—Ä–æ–π–∫–∏ –ø–µ—Ä—Å–æ–Ω–∞–∂–∞
        currentSpeed = 16,
        currentJump = 50,
    }
    
    ------------------------------------------------------------
    -- –ú–û–î–£–õ–¨: –ü–û–õ–ù–ê–Ø –°–ò–°–¢–ï–ú–ê –ù–ï–í–ò–î–ò–ú–û–°–¢–ò
    ------------------------------------------------------------
    local InvisibilitySystem = {}
    
    -- –•—Ä–∞–Ω–∏–ª–∏—â–µ –¥–ª—è –≤—Ä–µ–º–µ–Ω–Ω—ã—Ö –¥–∞–Ω–Ω—ã—Ö
    local hiddenAccessories = {}
    
    -- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Å–∫—Ä—ã—Ç–∏—è –í–°–ï–• UI —ç–ª–µ–º–µ–Ω—Ç–æ–≤ –ø–µ—Ä—Å–æ–Ω–∞–∂–∞
    function InvisibilitySystem.hideAllUI(character)
        if not character then return end
        
        -- –ò—â–µ–º –≤—Å–µ BillboardGui –∏ –ø–æ–¥–æ–±–Ω—ã–µ —ç–ª–µ–º–µ–Ω—Ç—ã
        for _, descendant in ipairs(character:GetDescendants()) do
            if descendant:IsA("BillboardGui") or descendant:IsA("SurfaceGui") then
                -- –°–æ—Ö—Ä–∞–Ω—è–µ–º –æ—Ä–∏–≥–∏–Ω–∞–ª—å–Ω—ã–µ –Ω–∞—Å—Ç—Ä–æ–π–∫–∏
                if not state.invisibilityData.originalUIPositions[descendant] then
                    state.invisibilityData.originalUIPositions[descendant] = {
                        StudsOffset = descendant.StudsOffset,
                        Enabled = descendant.Enabled,
                        AlwaysOnTop = descendant.AlwaysOnTop
                    }
                end
                
                -- –°–º–µ—â–∞–µ–º –∑–∞ —ç–∫—Ä–∞–Ω
                descendant.StudsOffset = Vector3.new(-config.invisibilityOffsetDistance, 0, 0)
                descendant.Enabled = false
                descendant.AlwaysOnTop = false
                
                -- –î–µ–ª–∞–µ–º –≤—Å–µ –≤–Ω—É—Ç—Ä–µ–Ω–Ω–∏–µ —ç–ª–µ–º–µ–Ω—Ç—ã –ø—Ä–æ–∑—Ä–∞—á–Ω—ã–º–∏
                for _, uiElement in ipairs(descendant:GetDescendants()) do
                    if uiElement:IsA("TextLabel") or uiElement:IsA("TextButton") then
                        uiElement.TextTransparency = 1
                        uiElement.BackgroundTransparency = 1
                    end
                end
            end
        end
        
        -- –¢–∞–∫–∂–µ —Å–∫—Ä—ã–≤–∞–µ–º –ª—é–±—ã–µ ParticleEmitter (—á–∞—Å—Ç–∏—Ü—ã)
        for _, particle in ipairs(character:GetDescendants()) do
            if particle:IsA("ParticleEmitter") then
                particle.Enabled = false
            end
        end
    end
    
    -- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Å–∫—Ä—ã—Ç–∏—è 3D –º–æ–¥–µ–ª–∏
    function InvisibilitySystem.hideModel(character)
        if not character then return end
        
        for _, part in ipairs(character:GetDescendants()) do
            if part:IsA("BasePart") then
                -- –°–æ—Ö—Ä–∞–Ω—è–µ–º –æ—Ä–∏–≥–∏–Ω–∞–ª—å–Ω—ã–µ –∑–Ω–∞—á–µ–Ω–∏—è
                if not state.invisibilityData.originalTransparency[part] then
                    state.invisibilityData.originalTransparency[part] = part.Transparency
                end
                if not state.invisibilityData.originalCollisions[part] then
                    state.invisibilityData.originalCollisions[part] = part.CanCollide
                end
                
                -- –î–µ–ª–∞–µ–º –Ω–µ–≤–∏–¥–∏–º—ã–º –∏ —É–±–∏—Ä–∞–µ–º –∫–æ–ª–ª–∏–∑–∏–∏
                part.Transparency = 1
                part.CanCollide = false
                
                -- –¢–∞–∫–∂–µ —Å–∫—Ä—ã–≤–∞–µ–º –ª—é–±—ã–µ Decal/Texture
                for _, decal in ipairs(part:GetChildren()) do
                    if decal:IsA("Decal") then
                        decal.Transparency = 1
                    end
                end
            elseif part:IsA("Accessory") then
                -- –û–±—Ä–∞–±–∞—Ç—ã–≤–∞–µ–º –∞–∫—Å–µ—Å—Å—É–∞—Ä—ã
                local handle = part:FindFirstChild("Handle")
                if handle then
                    if not state.invisibilityData.originalTransparency[handle] then
                        state.invisibilityData.originalTransparency[handle] = handle.Transparency
                    end
                    handle.Transparency = 1
                    handle.CanCollide = false
                end
            end
        end
        
        -- –û–±—Ä–∞–±–æ—Ç–∫–∞ –æ–¥–µ–∂–¥—ã (–µ—Å–ª–∏ –µ—Å—Ç—å)
        local characterAppearance = character:FindFirstChildOfClass("Humanoid")
        if characterAppearance then
            for _, clothing in ipairs(characterAppearance:GetChildren()) do
                if clothing:IsA("Shirt") or clothing:IsA("Pants") or clothing:IsA("ShirtGraphic") then
                    clothing:Destroy() -- –£–¥–∞–ª—è–µ–º –æ–¥–µ–∂–¥—É
                    table.insert(hiddenAccessories, clothing:Clone())
                end
            end
        end
    end
    
    -- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø–µ—Ä–µ–º–µ—â–µ–Ω–∏—è —Ç–µ–ª–∞ –ø–æ–¥ –∑–µ–º–ª—é (–∞–ª—å—Ç–µ—Ä–Ω–∞—Ç–∏–≤–Ω—ã–π –º–µ—Ç–æ–¥)
    function InvisibilitySystem.moveUnderground(character)
        if not character then return end
        
        local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
        if humanoidRootPart then
            -- –°–æ—Ö—Ä–∞–Ω—è–µ–º –æ—Ä–∏–≥–∏–Ω–∞–ª—å–Ω—É—é –ø–æ–∑–∏—Ü–∏—é
            state.invisibilityData.undergroundPosition = humanoidRootPart.Position
            
            -- –ü–µ—Ä–µ–º–µ—â–∞–µ–º –ø–æ–¥ –∑–µ–º–ª—é
            local undergroundPos = Vector3.new(
                humanoidRootPart.Position.X,
                config.undergroundHeight,
                humanoidRootPart.Position.Z
            )
            
            -- –ü–ª–∞–≤–Ω–æ–µ –ø–µ—Ä–µ–º–µ—â–µ–Ω–∏–µ
            local tweenInfo = TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
            local tween = TweenService:Create(humanoidRootPart, tweenInfo, {Position = undergroundPos})
            tween:Play()
            
            -- –û—Ç–∫–ª—é—á–∞–µ–º –∫–æ–ª–ª–∏–∑–∏–∏ —É –≤—Å–µ—Ö —á–∞—Å—Ç–µ–π
            for _, part in ipairs(character:GetDescendants()) do
                if part:IsA("BasePart") then
                    part.CanCollide = false
                end
            end
        end
    end
    
    -- –§—É–Ω–∫—Ü–∏—è –≤–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∏—è –Ω–æ—Ä–º–∞–ª—å–Ω–æ–≥–æ —Å–æ—Å—Ç–æ—è–Ω–∏—è
    function InvisibilitySystem.restoreVisibility(character)
        if not character then return end
        
        -- –í–æ—Å—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º UI —ç–ª–µ–º–µ–Ω—Ç—ã
        for gui, originalData in pairs(state.invisibilityData.originalUIPositions) do
            if gui and gui.Parent then
                gui.StudsOffset = originalData.StudsOffset or Vector3.new(0, 3, 0)
                gui.Enabled = originalData.Enabled ~= false
                gui.AlwaysOnTop = originalData.AlwaysOnTop or true
            end
        end
        
        -- –í–æ—Å—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º –ø—Ä–æ–∑—Ä–∞—á–Ω–æ—Å—Ç—å –∏ –∫–æ–ª–ª–∏–∑–∏–∏
        for part, transparency in pairs(state.invisibilityData.originalTransparency) do
            if part and part.Parent then
                part.Transparency = transparency
            end
        end
        
        for part, canCollide in pairs(state.invisibilityData.originalCollisions) do
            if part and part.Parent then
                part.CanCollide = canCollide
            end
        end
        
        -- –í–æ–∑–≤—Ä–∞—â–∞–µ–º –∏–∑-–ø–æ–¥ –∑–µ–º–ª–∏
        local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
        if humanoidRootPart and state.invisibilityData.undergroundPosition then
            local tweenInfo = TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
            local tween = TweenService:Create(humanoidRootPart, tweenInfo, {Position = state.invisibilityData.undergroundPosition})
            tween:Play()
        end
        
        -- –í–æ—Å—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º –æ–¥–µ–∂–¥—É
        local characterAppearance = character:FindFirstChildOfClass("Humanoid")
        if characterAppearance and #hiddenAccessories > 0 then
            for _, clothing in ipairs(hiddenAccessories) do
                clothing:Clone().Parent = characterAppearance
            end
            hiddenAccessories = {}
        end
        
        -- –í–∫–ª—é—á–∞–µ–º —á–∞—Å—Ç–∏—Ü—ã
        for _, particle in ipairs(character:GetDescendants()) do
            if particle:IsA("ParticleEmitter") then
                particle.Enabled = true
            end
        end
        
        -- –û—á–∏—â–∞–µ–º —Å–æ—Ö—Ä–∞–Ω—ë–Ω–Ω—ã–µ –¥–∞–Ω–Ω—ã–µ
        state.invisibilityData.originalTransparency = {}
        state.invisibilityData.originalCollisions = {}
        state.invisibilityData.originalUIPositions = {}
        state.invisibilityData.undergroundPosition = nil
    end
    
    -- –û—Å–Ω–æ–≤–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è –ø–µ—Ä–µ–∫–ª—é—á–µ–Ω–∏—è –Ω–µ–≤–∏–¥–∏–º–æ—Å—Ç–∏
    function InvisibilitySystem.toggleInvisibility()
        local character = player.Character
        if not character then return end
        
        state.isInvisible = not state.isInvisible
        
        if state.isInvisible then
            -- –ê–ö–¢–ò–í–ê–¶–ò–Ø –ù–ï–í–ò–î–ò–ú–û–°–¢–ò
            print("[ASMenu] –ê–∫—Ç–∏–≤–∞—Ü–∏—è —Ä–∞—Å—à–∏—Ä–µ–Ω–Ω–æ–π –Ω–µ–≤–∏–¥–∏–º–æ—Å—Ç–∏...")
            
            -- –ú–µ—Ç–æ–¥ 1: –°–∫—Ä—ã–≤–∞–µ–º UI –∏ –º–æ–¥–µ–ª—å
            InvisibilitySystem.hideAllUI(character)
            InvisibilitySystem.hideModel(character)
            
            -- –ú–µ—Ç–æ–¥ 2: –î–æ–ø–æ–ª–Ω–∏—Ç–µ–ª—å–Ω–æ –ø–µ—Ä–µ–º–µ—â–∞–µ–º –ø–æ–¥ –∑–µ–º–ª—é (–æ–ø—Ü–∏–æ–Ω–∞–ª—å–Ω–æ)
            -- InvisibilitySystem.moveUnderground(character)
            
            -- –û–±–Ω–æ–≤–ª—è–µ–º UI –∫–Ω–æ–ø–∫–∏
            if UI.invisibilityButton then
                UI.invisibilityButton.Text = "üëÅÔ∏è –ù–µ–≤–∏–¥–∏–º–æ—Å—Ç—å (–ê–ö–¢–ò–í)"
                UI.invisibilityButton.BackgroundColor3 = Color3.fromRGB(0, 180, 0)
            end
            
            -- –°–æ–æ–±—â–µ–Ω–∏–µ –≤ –∫–æ–Ω—Å–æ–ª—å
            print("[ASMenu] –ù–µ–≤–∏–¥–∏–º–æ—Å—Ç—å –∞–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω–∞")
            print("[ASMenu] UI —ç–ª–µ–º–µ–Ω—Ç—ã —Å–º–µ—â–µ–Ω—ã –∑–∞ —ç–∫—Ä–∞–Ω")
            print("[ASMenu] –ú–æ–¥–µ–ª—å —Å–¥–µ–ª–∞–Ω–∞ –Ω–µ–≤–∏–¥–∏–º–æ–π")
            
        else
            -- –î–ï–ê–ö–¢–ò–í–ê–¶–ò–Ø –ù–ï–í–ò–î–ò–ú–û–°–¢–ò
            print("[ASMenu] –í–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∏–µ –≤–∏–¥–∏–º–æ—Å—Ç–∏...")
            
            InvisibilitySystem.restoreVisibility(character)
            
            if UI.invisibilityButton then
                UI.invisibilityButton.Text = "üëÅÔ∏è –†–∞—Å—à–∏—Ä–µ–Ω–Ω–∞—è –Ω–µ–≤–∏–¥–∏–º–æ—Å—Ç—å"
                UI.invisibilityButton.BackgroundColor3 = Color3.fromRGB(65, 65, 65)
            end
            
            print("[ASMenu] –í–∏–¥–∏–º–æ—Å—Ç—å –≤–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∞")
        end
    end
    
    -- –ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–æ–µ –ø—Ä–∏–º–µ–Ω–µ–Ω–∏–µ –ø—Ä–∏ —Å–º–µ–Ω–µ –ø–µ—Ä—Å–æ–Ω–∞–∂–∞
    player.CharacterAdded:Connect(function(character)
        if state.isInvisible then
            wait(0.5) -- –ñ–¥—ë–º –ø–æ–ª–Ω–æ–π –∑–∞–≥—Ä—É–∑–∫–∏ –ø–µ—Ä—Å–æ–Ω–∞–∂–∞
            InvisibilitySystem.hideAllUI(character)
            InvisibilitySystem.hideModel(character)
        end
    end)
    
    ------------------------------------------------------------
    -- –ú–û–î–£–õ–¨ UI: –°–û–ó–î–ê–ù–ò–ï –ò–ù–¢–ï–†–§–ï–ô–°–ê
    ------------------------------------------------------------
    local UI = {}
    
    -- –û—Å–Ω–æ–≤–Ω–æ–π ScreenGui
    UI.screenGui = Instance.new("ScreenGui")
    UI.screenGui.Name = "ASMenu"
    UI.screenGui.ResetOnSpawn = false
    UI.screenGui.Parent = playerGui
    
    -- –ì–ª–∞–≤–Ω–æ–µ –º–µ–Ω—é
    UI.mainFrame = Instance.new("Frame")
    UI.mainFrame.Name = "MainFrame"
    UI.mainFrame.Size = UDim2.new(0, config.menuWidth, 0, config.menuHeight)
    UI.mainFrame.Position = UDim2.new(0, 20, 0.5, -config.menuHeight/2)
    UI.mainFrame.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
    UI.mainFrame.BorderSizePixel = 0
    UI.mainFrame.Active = true
    UI.mainFrame.Selectable = true
    UI.mainFrame.Parent = UI.screenGui
    
    -- –ó–∞–∫—Ä—É–≥–ª–µ–Ω–∏–µ —É–≥–ª–æ–≤
    Instance.new("UICorner", UI.mainFrame).CornerRadius = UDim.new(0, 8)
    
    -- –ó–∞–≥–æ–ª–æ–≤–æ–∫
    UI.titleBar = Instance.new("Frame")
    UI.titleBar.Name = "TitleBar"
    UI.titleBar.Size = UDim2.new(1, 0, 0, config.titleBarHeight)
    UI.titleBar.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
    UI.titleBar.BorderSizePixel = 0
    UI.titleBar.Parent = UI.mainFrame
    
    -- –¢–µ–∫—Å—Ç –∑–∞–≥–æ–ª–æ–≤–∫–∞
    UI.titleText = Instance.new("TextLabel")
    UI.titleText.Text = "üì± ASMenu vF1"
    UI.titleText.Size = UDim2.new(1, -80, 1, 0)
    UI.titleText.Position = UDim2.new(0, 10, 0, 0)
    UI.titleText.BackgroundTransparency = 1
    UI.titleText.TextColor3 = Color3.fromRGB(255, 255, 255)
    UI.titleText.TextSize = 16
    UI.titleText.Font = Enum.Font.GothamBold
    UI.titleText.TextXAlignment = Enum.TextXAlignment.Left
    UI.titleText.Parent = UI.titleBar
    
    -- –ö–Ω–æ–ø–∫–∞ –∑–∞–∫—Ä—ã—Ç–∏—è
    UI.closeButton = Instance.new("TextButton")
    UI.closeButton.Name = "CloseButton"
    UI.closeButton.Size = UDim2.new(0, 30, 0, 30)
    UI.closeButton.Position = UDim2.new(1, -35, 0.5, -15)
    UI.closeButton.BackgroundColor3 = Color3.fromRGB(200, 50, 50)
    UI.closeButton.Text = "X"
    UI.closeButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    UI.closeButton.Font = Enum.Font.GothamBold
    UI.closeButton.TextSize = 14
    UI.closeButton.Parent = UI.titleBar
    
    -- –ö–æ–Ω—Ç–µ–π–Ω–µ—Ä –¥–ª—è –∫–Ω–æ–ø–æ–∫
    UI.contentFrame = Instance.new("ScrollingFrame")
    UI.contentFrame.Name = "ContentFrame"
    UI.contentFrame.Size = UDim2.new(1, 0, 1, -config.titleBarHeight)
    UI.contentFrame.Position = UDim2.new(0, 0, 0, config.titleBarHeight)
    UI.contentFrame.BackgroundTransparency = 1
    UI.contentFrame.BorderSizePixel = 0
    UI.contentFrame.ScrollBarThickness = 4
    UI.contentFrame.ScrollBarImageColor3 = Color3.fromRGB(100, 100, 100)
    UI.contentFrame.CanvasSize = UDim2.new(0, 0, 0, 0)
    UI.contentFrame.Parent = UI.mainFrame
    
    -- Layout –¥–ª—è –∫–Ω–æ–ø–æ–∫
    UI.contentListLayout = Instance.new("UIListLayout")
    UI.contentListLayout.Padding = UDim.new(0, 8)
    UI.contentListLayout.SortOrder = Enum.SortOrder.LayoutOrder
    UI.contentListLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
    UI.contentListLayout.Parent = UI.contentFrame
    
    -- –û—Ç—Å—Ç—É–ø—ã
    UI.contentPadding = Instance.new("UIPadding")
    UI.contentPadding.PaddingTop = UDim.new(0, 10)
    UI.contentPadding.PaddingBottom = UDim.new(0, 10)
    UI.contentPadding.PaddingLeft = UDim.new(0, 10)
    UI.contentPadding.PaddingRight = UDim.new(0, 10)
    UI.contentPadding.Parent = UI.contentFrame
    
    -- –ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏–π —Ä–∞–∑–º–µ—Ä –∫–æ–Ω—Ç–µ–Ω—Ç–∞
    UI.contentListLayout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
        UI.contentFrame.CanvasSize = UDim2.new(0, 0, 0, UI.contentListLayout.AbsoluteContentSize.Y + 20)
    end)
    
    -- –§—É–Ω–∫—Ü–∏—è —Å–æ–∑–¥–∞–Ω–∏—è –∫–Ω–æ–ø–æ–∫
    function UI.createButton(name, text, order, icon)
        local button = Instance.new("TextButton")
        button.Name = name
        button.Size = UDim2.new(1, -20, 0, 45)
        button.BackgroundColor3 = Color3.fromRGB(65, 65, 65)
        button.BorderSizePixel = 0
        button.TextColor3 = Color3.fromRGB(255, 255, 255)
        button.TextSize = 14
        button.Font = Enum.Font.Gotham
        button.Text = (icon or "") .. " " .. text
        button.LayoutOrder = order
        button.Parent = UI.contentFrame
        
        Instance.new("UICorner", button).CornerRadius = UDim.new(0, 6)
        
        -- –≠—Ñ—Ñ–µ–∫—Ç—ã –Ω–∞–≤–µ–¥–µ–Ω–∏—è
        button.MouseEnter:Connect(function()
            if button.BackgroundColor3 ~= Color3.fromRGB(0, 180, 0) then
                button.BackgroundColor3 = Color3.fromRGB(85, 85, 85)
            end
        end)
        
        button.MouseLeave:Connect(function()
            if button.BackgroundColor3 ~= Color3.fromRGB(0, 180, 0) then
                button.BackgroundColor3 = Color3.fromRGB(65, 65, 65)
            end
        end)
        
        return button
    end
    
    ------------------------------------------------------------
    -- –°–û–ó–î–ê–ù–ò–ï –ö–ù–û–ü–û–ö –ú–ï–ù–Æ
    ------------------------------------------------------------
    
    -- –û—Å–Ω–æ–≤–Ω—ã–µ —Ñ—É–Ω–∫—Ü–∏–∏
    UI.teleportButton = UI.createButton("TeleportButton", "–¢–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏—è", 1, "üìç")
    UI.noClipButton = UI.createButton("NoClipButton", "–•–æ–¥–∏—Ç—å —Å–∫–≤–æ–∑—å —Å—Ç–µ–Ω—ã", 2, "üëª")
    UI.flyButton = UI.createButton("FlyButton", "–ü–æ–ª—ë—Ç", 3, "‚úàÔ∏è")
    UI.noFallDamageButton = UI.createButton("NoFallDamageButton", "–ù–µ—Ç —É—Ä–æ–Ω–∞ –æ—Ç –ø–∞–¥–µ–Ω–∏—è", 4, "üõ°Ô∏è")
    
    -- –ù–ï–í–ò–î–ò–ú–û–°–¢–¨ (–≥–ª–∞–≤–Ω–∞—è –∫–Ω–æ–ø–∫–∞)
    UI.invisibilityButton = UI.createButton("InvisibilityButton", "–†–∞—Å—à–∏—Ä–µ–Ω–Ω–∞—è –Ω–µ–≤–∏–¥–∏–º–æ—Å—Ç—å", 5, "üëÅÔ∏è")
    
    -- ESP —Ñ—É–Ω–∫—Ü–∏–∏
    UI.espPlayersButton = UI.createButton("ESPPlayersButton", "ESP –ò–≥—Ä–æ–∫–æ–≤", 6, "üë§")
    UI.espNPCButton = UI.createButton("ESPNPCButton", "ESP –°—É—â–µ—Å—Ç–≤", 7, "üëπ")
    
    -- –û—Å–≤–µ—â–µ–Ω–∏–µ
    UI.lightButton = UI.createButton("LightButton", "–û—Å–≤–µ—Ç–∏—Ç—å –∫–∞—Ä—Ç—É", 8, "üí°")
    
    -- –ù–∞—Å—Ç—Ä–æ–π–∫–∏ –ø–µ—Ä—Å–æ–Ω–∞–∂–∞
    UI.speedButton = UI.createButton("SpeedButton", "–°–∫–æ—Ä–æ—Å—Ç—å: " .. state.currentSpeed, 9, "‚ö°")
    UI.jumpButton = UI.createButton("JumpButton", "–ü—Ä—ã–∂–æ–∫: " .. state.currentJump, 10, "ü¶ò")
    
    -- –í–∏–¥ –æ—Ç —Ç—Ä–µ—Ç—å–µ–≥–æ –ª–∏—Ü–∞
    UI.thirdPersonButton = UI.createButton("ThirdPersonButton", "–†–µ–∂–∏–º 3-–≥–æ –ª–∏—Ü–∞", 11, "üé•")
    
    -- –ö–Ω–æ–ø–∫–∞ —Å–≤–æ—Ä–∞—á–∏–≤–∞–Ω–∏—è –≤ –º–∞–ª–µ–Ω—å–∫—É—é –∫–Ω–æ–ø–æ—á–∫—É
    UI.minimizeButton = UI.createButton("MinimizeButton", "–°–≤–µ—Ä–Ω—É—Ç—å –≤ –∫–Ω–æ–ø–∫—É", 12, "üì±")
    
    ------------------------------------------------------------
    -- –ú–ê–õ–ï–ù–¨–ö–ê–Ø –ö–ù–û–ü–û–ß–ö–ê (–∫–æ–≥–¥–∞ –º–µ–Ω—é —Å–≤—ë—Ä–Ω—É—Ç–æ)
    ------------------------------------------------------------
    UI.smallButton = Instance.new("TextButton")
    UI.smallButton.Name = "SmallButton"
    UI.smallButton.Size = UDim2.new(0, 50, 0, 50)
    UI.smallButton.Position = UDim2.new(0, 20, 0.5, -25)
    UI.smallButton.BackgroundColor3 = Color3.fromRGB(0, 120, 215)
    UI.smallButton.BackgroundTransparency = 0.3
    UI.smallButton.Text = "üì±"
    UI.smallButton.TextSize = 20
    UI.smallButton.Visible = false
    UI.smallButton.Parent = UI.screenGui
    Instance.new("UICorner", UI.smallButton).CornerRadius = UDim.new(1, 0)
    
    ------------------------------------------------------------
    -- –ü–ï–†–ï–ú–ï–©–ï–ù–ò–ï –ú–ï–ù–Æ –ò –ö–ù–û–ü–ö–ò
    ------------------------------------------------------------
    local dragStartPosition, dragStartMousePosition
    
    local function startDragging(frame)
        state.isDragging = true
        dragStartPosition = frame.Position
        dragStartMousePosition = UserInputService:GetMouseLocation()
        
        -- –í–∏–∑—É–∞–ª—å–Ω–∞—è –æ–±—Ä–∞—Ç–Ω–∞—è —Å–≤—è–∑—å
        frame.BackgroundTransparency = frame == UI.smallButton and 0.1 or 0.05
    end
    
    local function updateDragging(frame)
        if not state.isDragging then return end
        
        local currentMousePosition = UserInputService:GetMouseLocation()
        local delta = currentMousePosition - dragStartMousePosition
        
        local newPosition = UDim2.new(
            dragStartPosition.X.Scale,
            dragStartPosition.X.Offset + delta.X,
            dragStartPosition.Y.Scale,
            dragStartPosition.Y.Offset + delta.Y
        )
        
        -- –û–≥—Ä–∞–Ω–∏—á–µ–Ω–∏–µ –≤ –ø—Ä–µ–¥–µ–ª–∞—Ö —ç–∫—Ä–∞–Ω–∞
        local screenSize = playerGui.AbsoluteSize
        local frameSize = frame.AbsoluteSize
        
        local xOffset = math.clamp(newPosition.X.Offset, 0, screenSize.X - frameSize.X)
        local yOffset = math.clamp(newPosition.Y.Offset, 0, screenSize.Y - frameSize.Y)
        
        frame.Position = UDim2.new(newPosition.X.Scale, xOffset, newPosition.Y.Scale, yOffset)
    end
    
    local function stopDragging(frame)
        state.isDragging = false
        frame.BackgroundTransparency = frame == UI.smallButton and 0.3 or 0
    end
    
    -- –ù–∞—Å—Ç—Ä–æ–π–∫–∞ –ø–µ—Ä–µ—Ç–∞—Å–∫–∏–≤–∞–Ω–∏—è –¥–ª—è –æ—Å–Ω–æ–≤–Ω–æ–≥–æ –º–µ–Ω—é
    UI.titleBar.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 or 
           input.UserInputType == Enum.UserInputType.Touch then
            startDragging(UI.mainFrame)
        end
    end)
    
    UserInputService.InputChanged:Connect(function(input)
        if state.isDragging then
            updateDragging(UI.mainFrame)
        end
    end)
    
    UserInputService.InputEnded:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 or 
           input.UserInputType == Enum.UserInputType.Touch then
            stopDragging(UI.mainFrame)
        end
    end)
    
    -- –ù–∞—Å—Ç—Ä–æ–π–∫–∞ –ø–µ—Ä–µ—Ç–∞—Å–∫–∏–≤–∞–Ω–∏—è –¥–ª—è –º–∞–ª–µ–Ω—å–∫–æ–π –∫–Ω–æ–ø–∫–∏
    UI.smallButton.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 or 
           input.UserInputType == Enum.UserInputType.Touch then
            startDragging(UI.smallButton)
        end
    end)
    
    ------------------------------------------------------------
    -- –§–£–ù–ö–¶–ò–û–ù–ê–õ–¨–ù–û–°–¢–¨ –ö–ù–û–ü–û–ö
    ------------------------------------------------------------
    
    -- –ó–∞–∫—Ä—ã—Ç–∏–µ –º–µ–Ω—é
    UI.closeButton.MouseButton1Click:Connect(function()
        UI.screenGui:Destroy()
    end)
    
    -- –°–≤–æ—Ä–∞—á–∏–≤–∞–Ω–∏–µ –≤ –º–∞–ª–µ–Ω—å–∫—É—é –∫–Ω–æ–ø–∫—É
    UI.minimizeButton.MouseButton1Click:Connect(function()
        UI.mainFrame.Visible = false
        UI.smallButton.Position = UI.mainFrame.Position
        UI.smallButton.Visible = true
        state.menuVisible = false
    end)
    
    -- –†–∞–∑–≤–æ—Ä–∞—á–∏–≤–∞–Ω–∏–µ –∏–∑ –º–∞–ª–µ–Ω—å–∫–æ–π –∫–Ω–æ–ø–∫–∏
    UI.smallButton.MouseButton1Click:Connect(function()
        if state.isDragging then return end
        
        UI.smallButton.Visible = false
        UI.mainFrame.Position = UI.smallButton.Position
        UI.mainFrame.Visible = true
        state.menuVisible = true
    end)
    
    -- –ù–ï–í–ò–î–ò–ú–û–°–¢–¨
    UI.invisibilityButton.MouseButton1Click:Connect(function()
        InvisibilitySystem.toggleInvisibility()
    end)
    
    -- NoClip (–∑–∞–≥–ª—É—à–∫–∞)
    UI.noClipButton.MouseButton1Click:Connect(function()
        state.isNoClip = not state.isNoClip
        UI.noClipButton.BackgroundColor3 = state.isNoClip and Color3.fromRGB(0, 180, 0) or Color3.fromRGB(65, 65, 65)
        UI.noClipButton.Text = state.isNoClip and "üëª NoClip (–í–ö–õ)" or "üëª –•–æ–¥–∏—Ç—å —Å–∫–≤–æ–∑—å —Å—Ç–µ–Ω—ã"
        print("NoClip:", state.isNoClip and "–í–∫–ª—é—á–µ–Ω" or "–í—ã–∫–ª—é—á–µ–Ω")
    end)
    
    -- –ü–æ–ª—ë—Ç (–∑–∞–≥–ª—É—à–∫–∞)
    UI.flyButton.MouseButton1Click:Connect(function()
        state.isFlying = not state.isFlying
        UI.flyButton.BackgroundColor3 = state.isFlying and Color3.fromRGB(0, 180, 0) or Color3.fromRGB(65, 65, 65)
        UI.flyButton.Text = state.isFlying and "‚úàÔ∏è –ü–æ–ª—ë—Ç (–í–ö–õ)" or "‚úàÔ∏è –ü–æ–ª—ë—Ç"
        print("–ü–æ–ª—ë—Ç:", state.isFlying and "–í–∫–ª—é—á–µ–Ω" or "–í—ã–∫–ª—é—á–µ–Ω")
    end)
    
    -- –¢–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏—è (–∑–∞–≥–ª—É—à–∫–∞)
    UI.teleportButton.MouseButton1Click:Connect(function()
        print("–ú–µ–Ω—é —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏–∏ –±—É–¥–µ—Ç –¥–æ–±–∞–≤–ª–µ–Ω–æ –≤ —Å–ª–µ–¥—É—é—â–µ–º –æ–±–Ω–æ–≤–ª–µ–Ω–∏–∏")
    end)
    
    ------------------------------------------------------------
    -- –ê–í–¢–û–ú–ê–¢–ò–ß–ï–°–ö–û–ï –û–ë–ù–û–í–õ–ï–ù–ò–ï –°–û–°–¢–û–Ø–ù–ò–Ø
    ------------------------------------------------------------
    RunService.RenderStepped:Connect(function()
        -- –ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–æ–µ –ø—Ä–∏–º–µ–Ω–µ–Ω–∏–µ NoClip
        if state.isNoClip and player.Character then
            for _, part in ipairs(player.Character:GetDescendants()) do
                if part:IsA("BasePart") then
                    part.CanCollide = false
                end
            end
        end
        
        -- –û–±–Ω–æ–≤–ª–µ–Ω–∏–µ —Å–∫–æ—Ä–æ—Å—Ç–∏ –∏ –ø—Ä—ã–∂–∫–∞
        if player.Character then
            local humanoid = player.Character:FindFirstChild("Humanoid")
            if humanoid then
                humanoid.WalkSpeed = state.currentSpeed
                humanoid.JumpPower = state.currentJump
            end
        end
    end)
    
    ------------------------------------------------------------
    -- –ò–ù–ò–¶–ò–ê–õ–ò–ó–ê–¶–ò–Ø –ü–ï–†–°–û–ù–ê–ñ–ê
    ------------------------------------------------------------
    player.CharacterAdded:Connect(function(character)
        -- –ü—Ä–∏–º–µ–Ω—è–µ–º –Ω–µ–≤–∏–¥–∏–º–æ—Å—Ç—å, –µ—Å–ª–∏ –æ–Ω–∞ –∞–∫—Ç–∏–≤–Ω–∞
        if state.isInvisible then
            task.wait(0.5)
            InvisibilitySystem.hideAllUI(character)
            InvisibilitySystem.hideModel(character)
        end
        
        -- –ü—Ä–∏–º–µ–Ω—è–µ–º –¥—Ä—É–≥–∏–µ –∞–∫—Ç–∏–≤–Ω—ã–µ —Ñ—É–Ω–∫—Ü–∏–∏
        if state.isNoClip then
            for _, part in ipairs(character:GetDescendants()) do
                if part:IsA("BasePart") then
                    part.CanCollide = false
                end
            end
        end
    end)
    
    ------------------------------------------------------------
    -- –ó–ê–í–ï–†–®–ï–ù–ò–ï –ó–ê–ì–†–£–ó–ö–ò
    ------------------------------------------------------------
    print("========================================")
    print("ASMenu vF1 by AbikusSudo - –£—Å–ø–µ—à–Ω–æ –∑–∞–≥—Ä—É–∂–µ–Ω!")
    print("–ü–∞—Ä–æ–ª—å: AbikusSudo")
    print("========================================")
    print("–§—É–Ω–∫—Ü–∏–∏:")
    print("1. –†–∞—Å—à–∏—Ä–µ–Ω–Ω–∞—è –Ω–µ–≤–∏–¥–∏–º–æ—Å—Ç—å (UI + –º–æ–¥–µ–ª—å)")
    print("2. –ü–µ—Ä–µ–º–µ—â–∞–µ–º–æ–µ –º–µ–Ω—é –∏ –∫–Ω–æ–ø–∫–∞")
    print("3. –°–≤–æ—Ä–∞—á–∏–≤–∞–Ω–∏–µ –≤ –º–∞–ª–µ–Ω—å–∫—É—é –∫–Ω–æ–ø–∫—É")
    print("4. NoClip, –ü–æ–ª—ë—Ç, ESP")
    print("5. –ù–∞—Å—Ç—Ä–æ–π–∫–∏ —Å–∫–æ—Ä–æ—Å—Ç–∏ –∏ –ø—Ä—ã–∂–∫–∞")
    print("========================================")
    print("–ò—Å–ø–æ–ª—å–∑—É–π—Ç–µ –∫–Ω–æ–ø–∫—É 'üëÅÔ∏è' –¥–ª—è –∞–∫—Ç–∏–≤–∞—Ü–∏–∏ –Ω–µ–≤–∏–¥–∏–º–æ—Å—Ç–∏")
    print("–ù–∏–∫–∏ –∏ healthbar –±—É–¥—É—Ç —Å–º–µ—â–µ–Ω—ã –∑–∞ —ç–∫—Ä–∞–Ω")
    print("========================================")
end
